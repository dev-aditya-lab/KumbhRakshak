    "@react-navigation/native-stack": "^7.3.21",
    "expo": "^53.0.20",
    "expo-status-bar": "~2.2.3",
    "i18next": "^25.3.2",
    "nativewind": "latest",
    "react": "19.0.0",
    "react-i18next": "^15.6.1",
    "react-native": "0.79.5",
